{
  "name": "mazer-front-end",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.3.0",
    "lodash": "^4.17.4",
    "mazer-shared": "0.0.36",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-history": "^0.18.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2"
  },
  "scripts": {
    "build-css": "stylus-chokidar src/ -o src/",
    "watch-css": "npm run build-css && stylus-chokidar src/ -o src/ -w",
    "start-js": "NODE_ENV=development BABEL_ENV=development node scripts/start.js",
    "start": "run-p watch-css start-js",
    "build": "npm run build-css && NODE_ENV=production BABEL_ENV=production node scripts/build.js",
    "test": "NODE_ENV=test BABEL_ENV=test tape -r babel-register tests/**/*.js --inspect",
    "test-faucet": "NODE_ENV=test BABEL_ENV=test tape -r babel-register tests/**/*.js --inspect | faucet"
  },
  "devDependencies": {
    "fs-extra": "3.0.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "whatwg-fetch": "2.0.3",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "3.0.0",
    "faucet": "0.0.1",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "2.29.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "react-dev-utils": "^3.1.0",
    "react-error-overlay": "^1.0.10",
    "redux-devtools": "^3.4.0",
    "source-map": "^0.5.7",
    "style-loader": "0.18.2",
    "stylus-chokidar": "0.0.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "tape": "^4.8.0",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
